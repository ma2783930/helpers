{
    "name": "ma2783930/helpers",
    "description": "Laravel helpers package",
    "type": "library",
    "homepage": "https://github.com/ma2783930/helpers",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Helpers\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Helpers\\Providers\\HelpersServiceProvider"
            ],
            "aliases": {
                "BinaryFile": "Helpers\\Facades\\BinaryFile"
            }
        }
    },
    "authors": [
        {
            "name": "m.alizadeh",
            "email": "majid.alizadeh22@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "^8|^9|^10.12",
        "hekmatinasser/verta": "^8.3",
        "ma2783930/eloquent-traits": "^1.0"
    }
}
